{
  "name": "@cinira-llc/demo-model",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "repository": {
    "url": "git+https://github.com/cinira-llc/demo-model.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-cjs.json && tsc -p ./tsconfig-types.json",
    "clean": "rm -rf ./build ./dist",
    "format": "prettier --write \"src/main/ts/**/*.ts\"",
    "lint": "eslint ./src/main/ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && ./gradlew -x test -Pversion=$npm_package_version publish",
    "test": "jest"
  },
  "files": [
    "./dist"
  ],
  "keywords": [],
  "author": "Matt Jensen <mjensen@cinira.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "immer": ">=10.0.0",
    "lodash": ">=4.17.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "types": "./dist/esm/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./build/report",
          "outputName": "test-results.xml"
        }
      ]
    ]
  }
}
